{
    "name": "hesabro/helpers",
    "version": "1.0.4",
    "description": "Hesabro helpers module",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Moslem Tayari",
            "email": "avapardazmota@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "kartik-v/yii2-grid": "@dev"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "hesabro\\helpers\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "bootstrap": "hesabro\\helpers\\Bootstrap"
    }
}
